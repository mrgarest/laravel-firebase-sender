{
    "name": "mrgarest/laravel-firebase-sender",
    "description": "Laravel library for sending notifications with Firebase Cloud Messaging (FCM)",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "Garest",
        "MrGarest",
        "google",
        "laravel",
        "firebase",
        "fcm",
        "notification",
        "php"
    ],
    "authors": [
        {
            "name": "Garest",
            "email": "olehhoncharenko98@gmail.com",
            "homepage": "https://github.com/mrgarest"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0|^11.0",
        "google/auth": "^1.40"
    },
    "autoload": {
        "psr-4": {
            "MrGarest\\FirebaseSender\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrGarest\\FirebaseSender\\FirebaseSenderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
